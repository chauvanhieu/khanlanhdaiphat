<!-- Booking Start -->
<div class="container-fluid my-5 px-0">
     <div class="video wow fadeInUp" data-wow-delay="0.1s">
          <h1 class="text-white mb-4">Dịch vụ chuyên nghiệp</h1>
          <h3 class="text-white mb-0">Hổ trợ khách hàng lên ý tưởng và đặt số lượng không giới hạn</h3>
     </div>
     <div class="container position-relative wow fadeInUp" data-wow-delay="0.0000001s" style="margin-top: -6rem;">
          <div class="row justify-content-center">
               <div class="col-lg-8">
                    <div class="bg-light text-center p-5">
                         <h1 class="mb-4">Liên hệ cho chúng tôi để đặt mẫu</h1>
                         <form>
                              <div class="row g-3">
                                   <div class="col-12 col-sm-6">
                                        <input type="text" class="form-control border-0" placeholder="Tên đầy đủ"
                                             style="height: 55px;">
                                   </div>
                                   <div class="col-12 col-sm-6">
                                        <input type="email" class="form-control border-0" placeholder="Email"
                                             style="height: 55px;">
                                   </div>
                                   <div class="col-12 col-sm-6">
                                        <input type="email" class="form-control border-0" placeholder="Số điện thoại"
                                             style="height: 55px;">
                                   </div>
                                   <div class="col-12 col-sm-6">
                                        <input type="email" class="form-control border-0" placeholder="Địa chỉ"
                                             style="height: 55px;">
                                   </div>
                                   <div class="col-12">
                                        <textarea class="form-control border-0"
                                             placeholder="Mô tả về mẫu bạn muốn đặt"></textarea>
                                   </div>
                                   <div class="col-12">
                                        <button class="btn btn-primary w-100 py-3" type="submit">Gửi cho chúng
                                             tôi</button>
                                   </div>
                              </div>
                         </form>
                    </div>
               </div>
          </div>
     </div>
</div>
<!-- Booking End -->